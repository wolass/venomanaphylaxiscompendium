journal = {Seminars in Cardiothoracic and Vascular Anesthesia},
keywords = {cardiopulmonary bypass,critical care,intensive care unit,therapeutic hypothermia,transesophageal echocardiography},
mendeley-groups = {Refractory anaphylaxis},
month = {mar},
number = {1},
pages = {66--70},
pmid = {25552268},
title = {{Successful Rescue of an Adult With Refractory Anaphylactic Shock and Abdominal Compartment Syndrome With Venoarterial Extracorporeal Membrane Oxygenation and Bedside Laparotomy}},
dninclude = {http://journals.sagepub.com/doi/10.1177/1089253214564192},
volume = {19},
