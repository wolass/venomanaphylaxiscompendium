pages = {301--305},
pmid = {26256577},
publisher = {American College of Allergy, Asthma {\&} Immunology},
title = {{Addressing barriers to emergency anaphylaxis care: from emergency medical services to emergency department to outpatient follow-up}},
dninclude = {http://dx.doi.org/10.1016/j.anai.2015.07.008 http://linkinghub.elsevier.com/retrieve/pii/S1081120615004548},
volume = {115},
