pmid = {28378321},
title = {{Serum levels of 9$\alpha$,11$\beta$-PGF {\textless}sub{\textgreater}2{\textless}/sub{\textgreater} and apolipoprotein A1 achieve high predictive power as biomarkers of anaphylaxis}},
dninclude = {http://www.ncbi.nlm.nih.gov/pubmed/28378321 http://doi.wiley.com/10.1111/all.13176},
